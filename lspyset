#!/usr/bin/env eepython
##get intersection of two sets
set1 = set([0,2,4,6,8])
set2 = set((0,3,6,9))
print set1 & set2
print set1.intersection(set2)

##get difference of two sets
set1 = set([0,2,4,6,8])
set2 = set((0,3,6,9))
print set1 - set2
print set1.difference(set2)

##get union of two sets
set1 = set([0,2,4,6,8])
set2 = set((0,3,6,9))
print set1 | set2
print set1.union(set2)
##below not supported
#print set1 + set2
