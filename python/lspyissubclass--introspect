#!/usr/bin/env eepython
##
class MyObject():
   def __init__(self):
      pass
class Wall(MyObject):
   def __init__(self):
      pass
import inspect; print inspect.getmro(Wall)
print issubclass(MyObject, MyObject)
print issubclass(MyObject, Wall)
print issubclass(Wall, MyObject)
print issubclass(Wall, Wall)
